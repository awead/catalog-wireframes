<div class="hidden">
  <%= next_frame "results" %>
  <%= previous_frame "video" %>
</div>
  
<div class="col-sm-4">
  <div class="well">
    <%= render :partial => "catalog/partials/facets.html.erb" %>
  </div>
</div>

<div class="col-sm-8">
  <div class="row">
    <div class="col-xs-12">
      <h1>Search</h1>
      <p>Search the catalog for books, music, magazines, and archival collections</p>
      <form class="form-search">
        <input type="text" class="input-medium search-query" placeholder="Search in"> in
        <select>
          <option>All fields</option>
          <option>Title</option>
          <option>Author</option>
          <option>Keyword</option>
          <option>Call number</option>
        </select>
      </form>
      <span class="block"><%= link_to "How to use the catalog" %></span>
      <span class="block"><%= link_to "Search tips" %></span>
      <span class="block"><%= link_to "Login" %></span>
      <span class="block"><%= link_to "Advanced Search" %></span>
    </div>
  </div>
  <div class="row">
    <div class="col-xs-12">
      <h1>Browse</h1>
      <span class="block"><%= link_to "Archival Colletions" %></span>
      <span class="block"><%= link_to "Research Guides" %></span>
      <span class="block"><%= link_to "Databases" %></span>
      <span class="block"><%= link_to "List of Inductees" %></span>
      <span class="block"><%= link_to "New Aquisitions" %></span>
    </div>
  </div>
</div>
